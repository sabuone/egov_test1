<decorators defaultdir="/WEB-INF/jsp/decorators">
	<excludes>
		<pattern>*.html</pattern>
	</excludes>

	<decorator name="page" page="page_layout.jsp">
		<pattern>/*</pattern>
	</decorator>

	<decorator name="popup" page="popup_layout.jsp">
		<pattern>/popup.do</pattern>
	</decorator>

</decorators>